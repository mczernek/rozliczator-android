<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Application theme. -->
    <style name="ChujTheme" parent="@android:style/Theme.Holo.Light">
        <item name="android:actionBarStyle">@style/ActionBarTheme</item>
    </style>
    
    <style name="SplashTheme" parent="@android:style/Theme.Holo.Light">
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">#ffffff</item>
    </style>
    
    <style name="ActionBarTheme" parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:background">#30c27e</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:height">50dp</item>
        <item name="android:logo">@drawable/title</item>
    </style>
    
    <style name="ActionBarActivity">
        <item name="android:background">#ffffff</item>
    </style>
    
    <style name="ListViewStyle">
    </style>
    
    <style name="TextViewStyle">
    </style>

</resources>